@startuml

Enum Cat_qua{

ECONOMICO
LUXO

}

Enum Status{

ATIVO
CANCELADO

}

class Nodo{

- Nodo direita
- Nodo esquerda
- Cliente cliente
- Quarto quarto
- Status situacao

 + Nodo(Cliente cli, Quarto qua)

}


class Quarto{
- int num_quarto
- boolean disponivel
- cat_qua categoria
- LocalDateTime DataIniOcup 
- LocalDateTime DataFimOcup
}


class Cliente{
 - int id_cliente
 - String cpf
 - String nome
 

}

class Reserva{
 
 - Nodo raiz
 
 + checkout()
 + checkin()
 + reservar()
 + cancelarReserva()
 
 
 
 }
 
 class Registro{
    
    - Nodo raiz
    
    + inserirRegistro()
    + mostrarRegistro()
 
 }
 
 Cat_qua -down Quarto
 Status - Nodo
 Quarto --* Nodo
 Cliente --* Nodo
 Nodo --* Reserva

 
@enduml
